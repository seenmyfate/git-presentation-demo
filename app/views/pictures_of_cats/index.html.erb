<h1>Listing pictures_of_cats</h1>

<table>
  <tr>
    <th>Image</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pictures_of_cats.each do |pictures_of_cat| %>
  <tr>
    <td><%= pictures_of_cat.image %></td>
    <td><%= link_to 'Show', pictures_of_cat %></td>
    <td><%= link_to 'Edit', edit_pictures_of_cat_path(pictures_of_cat) %></td>
    <td><%= link_to 'Destroy', pictures_of_cat, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

